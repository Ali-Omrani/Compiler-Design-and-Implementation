actor Math<10>
	receiver calcSumUpToZero(int[10] array)
		int sum = 0
		foreach element in array
			if element <> 0
				sum = sum + element
			else
				break
				break
			end
			break
		end
		break
		sender << calcSumUpToZeroCompleted(sum)
	end
end
